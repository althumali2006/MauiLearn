<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiApp1.NewHome"
             Title="NewHome">
    <VerticalStackLayout>
        <Label Text="Enter your name here"/>
        <Entry Placeholder="Name..."  x:Name="NameInput"/>
        <Button Text="Push" WidthRequest="150" x:Name="BtnPush" Clicked="BtnPush_Clicked" HorizontalOptions="Center"/>
    </VerticalStackLayout>
</ContentPage>