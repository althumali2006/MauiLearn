<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiApplvl2.Pages.InsertPage1"
             Title="InsertPage1">
    <ScrollView>
        <VerticalStackLayout Spacing="10">
            <Entry Placeholder="Enter Name..." x:Name="NameEntry"/>
            <Entry Placeholder="Enter Age..." x:Name="AgeEntry"/>
            <Entry Placeholder="Enter Gender..." x:Name="GenderEntry"/>
            <Button Text="Insert" x:Name="BtnInsert" Clicked="BtnInsert_Clicked" WidthRequest="100"/>
        </VerticalStackLayout>
    </ScrollView>
</ContentPage>